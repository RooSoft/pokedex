<.header>
  Pokemons
</.header>

<.table
  id="pokemons"
  rows={@streams.pokemons}
  row_click={fn {_id, pokemon} -> JS.navigate(~p"/pokemons/#{pokemon}") end}
>
  <:col :let={{_id, pokemon}} label="ID"><%= pokemon.id %></:col>
  <:col :let={{_id, pokemon}} label="Name"><%= get_name(pokemon) %></:col>
  <:col label="Other">other</:col>
</.table>
